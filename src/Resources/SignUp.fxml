<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="501.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Foker.Game.Controllers.SignupController">
   <children>
      <Pane fx:id="Pane" layoutX="6.0" layoutY="8.0" prefHeight="486.0" prefWidth="653.0" styleClass="mainFxmlClass" stylesheets="@CSS.css">
         <effect>
            <DropShadow blurType="GAUSSIAN" color="#ee0909" spread="0.69" />
         </effect>
         <children>
            <Button fx:id="Btn_signup" layoutX="226.0" layoutY="363.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0" styleClass="orangeButton" stylesheets="@CSS.css" text="Sign Up">
               <font>
                  <Font name="Calibri Light" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="Txt_Pname" alignment="CENTER" layoutX="43.0" layoutY="183.0" prefHeight="33.0" prefWidth="283.0" promptText="Player Name">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <Label layoutX="264.0" layoutY="87.0" styleClass="subheader" text="Sign Up with us" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="202.0" layoutY="14.0" styleClass="mainheader" text="Foker Game" textAlignment="CENTER" textFill="WHITE" />
            <TextField fx:id="Txt_Pemail" alignment="CENTER" layoutX="346.0" layoutY="183.0" prefHeight="30.0" prefWidth="260.0" promptText="E-mail Address">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="Txt_Age" alignment="CENTER" layoutX="43.0" layoutY="229.0" prefHeight="33.0" prefWidth="283.0" promptText="Age">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <ComboBox fx:id="Cmb_Country" layoutX="346.0" layoutY="229.0" prefHeight="33.0" prefWidth="260.0" promptText="Country" />
            <PasswordField fx:id="Pwd" alignment="CENTER" layoutX="43.0" layoutY="270.0" prefHeight="33.0" prefWidth="283.0" promptText="Password">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </PasswordField>
            <Button fx:id="Btn_Back" layoutX="4.0" layoutY="436.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="91.0" styleClass="backButton" text="BACK" />
            <Button fx:id="Btn_Exit" layoutX="577.0" layoutY="436.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="73.0" styleClass="exitButton" text="EXIT" />
         </children>
      </Pane>
   </children>
</AnchorPane>
